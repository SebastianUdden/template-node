    "prebuild": "concurrently \"git add -A\" \"git commit -am \"Updated master branch\"\"",
    "build": "concurrently \"git push\" \"git push heroku master\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SebastianUdden/template-node.git"
  },
