  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SebastianUdden/template-node/issues"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "node_modules/**"
    ],
    "verbose": false
  },
  "homepage": "https://github.com/SebastianUdden/template-node#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "mongoose": "^5.0.6",
    "nodemon": "^1.14.12"
